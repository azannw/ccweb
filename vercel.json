  {
    "version": 2,
    "name": "ultra-premium-agency",
    "builds": [
      {
        "src": "**/*",
        "use": "@vercel/static"
      }
    ],
    "headers": [
      {
        "source": "/(.*)\\.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=3600"
          },
          {
            "key": "X-Content-Type-Options",
            "value": "nosniff"
          },
          {
            "key": "X-Frame-Options",
            "value": "DENY"
          },
          {
            "key": "X-XSS-Protection",
            "value": "1; mode=block"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
